<div class="row">
	<h1>職種変更または異動</h1>
	<form class="form-horizontal">
		<div class="form-group">
			<div class="agreement_no"></div>
		</div>
		<div class="form-group">
			<label class="col-md-2 control-label" for="cster_emply_cd">社員番号</label>
			<div class="col-md-3">
				<input type="text" id="cster_emply_cd" name="cster_emply_cd" class="form-control input-sm" maxlength="" placeholder="" />
			</div>
			<label class="col-md-1 control-label" for="werer_name">着用者名</label>
			<div class="col-md-3">
				<input type="text" id="werer_name" name="werer_name" class="form-control input-sm" maxlength="" placeholder="" />
			</div>
		</div>
		<div class="form-group">
			<label class="col-md-2 control-label" for="sex_kbn">性別</label>
			<div class="col-md-2">
				<div class="sex_kbn"></div>
			</div>
		</div>
		<div class="form-group">
			<div class="section"></div>
		</div>
		<div class="form-group">
			<label class="col-md-2 control-label" for="job_type">貸与パターン</label>
			<div class="col-md-3">
				<div class="job_type"></div>
			</div>
		</div>
		<div class="form-group" style="margin-bottom: 60px">
			<div class="col-md-2 col-md-push-7">
				<input type="hidden" class="ui_type" id="ui_type" name="ui_type" value="history">
				<a type="button" data-page="1" class="btn btn-primary btn-sm btn-block search" title="検索" id="button1" style="display: none;">検索</a>
			</div>
		</div>
        <!-- モーダル -->
        <div class="modal fade" id="myModalAlert" tabindex="-1" role="dialog" aria-labelledby="myModalAlertLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                    </div>
                    <div class="modal-body">
                        <p id="alert_txt"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" id="alert_ok"  class="btn btn-primary" data-dismiss="modal">OK</button>
                    </div>
                </div>
            </div>
        </div>
		<!-- 確認モーダル -->
		<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
			<div class="modal-dialog" role="document">
				<div class="modal-content">
					<div class="modal-header">
					</div>
					<div class="modal-body">
						<p id="confirm_txt"></p>
					</div>
					<div class="modal-footer">
						<button type="button" id="btn_cancel" class="btn btn-default" data-dismiss="modal">いいえ</button>
						<button type="button" id="btn_ok"  class="btn btn-primary">はい</button>
					</div>
				</div>
			</div>
		</div>

	</form>
	<hr>
